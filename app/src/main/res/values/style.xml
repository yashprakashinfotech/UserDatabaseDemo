<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="text_14sp_bold">
    <item name="android:textStyle">bold</item>
    <item name="android:textColor">@color/black</item>
    <item name="android:textSize">14sp</item>
    </style>

    <style name="text_24_bold">

        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">24sp</item>

    </style>


    <style name="round_button_Style">
        <item name="cornerRadius">25dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="textAllCaps">false</item>
        <item name="elevation">50dp</item>
        <item name="backgroundTint">@color/blue</item>

    </style>

    <style name="common_edit_text">

        <item name="android:layout_marginTop">5dp</item>
        <item name="android:ems">10</item>
        <item name="android:enabled">false</item>
        <item name="android:padding">10dp</item>
        <item name="paddingStart">25dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:focusable">true</item>
        <item name="android:drawablePadding">12dp</item>

    </style>

</resources>